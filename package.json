{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "testEnvironment": "jest-environment-node",
  "scripts": {
    "build": "tsc",
    "start": "tsc & node .",
    "dev": "tsc -w & cross-env NODE_ENV=development nodemon .",
    "migrations:dev": "cross-env NODE_ENV=development node ./migrate.js",
    "migrations:dev:up": "npm run migrations:dev -- up",
    "migrations:dev:down": "npm run migrations:dev -- down",
    "migrations:dev:pending": "npm run migrations:dev -- pending",
    "migrations:create": "npm run migrations:dev -- create",
    "lint": "eslint src --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "format": "npx prettier --write src/**/*.ts",
    "test:unit": "cross-env NODE_ENV=development jest --coverage",
    "test:coverage": "jest --coverage -c"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "globals": {
      "ts-jest": {
        "useESM": true,
        "diagnostics": false
      }
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-request-id": "^1.4.1",
    "express-validator": "^6.12.2",
    "faker": "^5.5.3",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.3",
    "mysql2": "^2.3.3",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pino": "^7.0.0-rc.8",
    "pino-pretty": "^7.0.1",
    "redis": "^4.2.0",
    "sequelize": "^6.6.5",
    "ts-node": "^10.9.1",
    "umzug": "^3.1.1"
  },
  "devDependencies": {
    "@jest/globals": "^28.1.3",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-request-id": "^1.4.3",
    "@types/express-validator": "^3.0.0",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.10.1",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/redis": "^2.8.32",
    "@types/stoppable": "^1.1.1",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fastify": "^4.3.0",
    "jest": "^28.1.3",
    "node-mocks-http": "^1.11.0",
    "nodemon": "^2.0.12",
    "sequelize-cli": "^6.4.1",
    "stoppable": "^1.1.0",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "typescript": "^4.4.3"
  }
}
